{
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/heading": {
				"spacing": {
					"margin": true,
					"padding": true
				}
			}
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#f9f9f9",
					"name": "Pale BG",
					"slug": "pale-bg"
				},
				{
					"color": "#F0F0F0",
					"name": "Grey",
					"slug": "grey"
				},
				{
					"color": "#E4F4FA",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#1D3557",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#457B9D",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#A8DADC",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#E63946",
					"name": "Dark Red",
					"slug": "darkred"
				}
			]
		},
		"layout": {
			"contentSize": "600px",
			"wideSize": "100vw"
		},
		"shadow": {
			"defaultPresets": true,
			"presets": [
				{
					"name": "Natural",
					"shadow": "6px 6px 9px rgba(0, 0, 0, 0.2)",
					"slug": "natural"
				},
				{
					"name": "Deep",
					"shadow": "12px 12px 50px rgba(0, 0, 0, 0.4)",
					"slug": "deep"
				},
				{
					"name": "Sharp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 0.2)",
					"slug": "sharp"
				},
				{
					"name": "Outlined",
					"shadow": "6px 6px 0px -3px rgba(255, 255, 255, 1), 6px 6px rgba(0, 0, 0, 1)",
					"slug": "outlined"
				},
				{
					"name": "Crisp",
					"shadow": "6px 6px 0px rgba(0, 0, 0, 1)",
					"slug": "crisp"
				}
			]
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Default",
					"slug": "body"
				}
			],
			"fontSizes": [
				{
					"name": "xSmall",
					"size": ".8rem",
					"slug": "xsmall"
				},
				{
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.2rem",
						"min": "1.2rem"
					},
					"name": "Normal",
					"size": "1.2rem",
					"slug": "normal"
				},
				{
					"fluid": {
						"max": "1.75rem",
						"min": "1.5rem"
					},
					"name": "Medium",
					"size": "1.5rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.5rem"
					},
					"name": "Large",
					"size": "2rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "52px",
						"min": "32px"
					},
					"name": "Extra Large",
					"size": "52px",
					"slug": "x-large"
				},
				{
					"name": "Fuck Large",
					"size": "8rem",
					"slug": "f-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textAlign": true,
			"textDecoration": true,
			"textTransform": true,
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": {
						"bottomLeft": "0px",
						"bottomRight": "0px",
						"topLeft": "0px",
						"topRight": "0px"
					}
				}
			},
			"core/code": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--body)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			},
			"core/group": {
				"css": "position:relative",
				"spacing": {
					"padding": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				}
			},
			"core/heading": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"elements": {
					"link": {
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				},
				"spacing": {
					"margin": {
						"bottom": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "var(--wp--preset--spacing--40)",
						"top": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--white)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--pale-bg)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontWeight": "100"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "24px",
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontStyle": "normal",
			"fontWeight": "100",
			"lineHeight": "1.5"
		}
	},
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.6/theme.json"
}